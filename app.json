{
  "expo": {
    "name": "fluentfox",
    "slug": "fluentfox",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fluentfox",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dralsallum.fluentfox",
      "entitlements": {
        "com.apple.developer.in-app-payments": [
          "merchant.com.dralsallum.fluentfox"
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSPhotoLibraryUsageDescription": "This app requires access to your photo library to upload images.",
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "your-ad-network-id"
          }
        ]
      }
    },
    "android": {
      "package": "com.dralsallum.fluentfox",
      "versionCode": 4,
      "playBilling": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "jsEngine": "jsc"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "react-native-iap"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7a90a210-7db1-451e-a07f-293fe3a353c2"
      }
    }
  }
}
